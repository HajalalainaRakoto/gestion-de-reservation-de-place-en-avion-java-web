    <url-pattern>/supprimer/reservation</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DataReservation</servlet-name>
    <servlet-class>com.example.Controlleur.Reservation.Data</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DataReservation</servlet-name>
    <url-pattern>/reservation</url-pattern>
  </servlet-mapping>

  <!-- voyageurs -->

  <servlet>
    <servlet-name>Voyageurs</servlet-name>
    <servlet-class>com.example.Controlleur.Autres.Voyageurs</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Voyageurs</servlet-name>
    <url-pattern>/voyageurs</url-pattern>
  </servlet-mapping>

</web-app>
